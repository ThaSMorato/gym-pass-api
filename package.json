{
  "name": "solid-api",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "prisma": "prisma",
    "prisma:type": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "studio": "prisma studio",
    "pretest:e2e": "npm run test:cpe && npm run test:install-pe",
    "test:cpe": "npm link ./prisma/vitest-environment-prisma",
    "test:install-pe": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http"
  },
  "keywords": [],
  "author": "Thales Silva Morato <ThaSMorato>",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.10.5",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.1.0",
    "@vitest/ui": "1.1.0",
    "eslint": "8.56.0",
    "supertest": "6.3.3",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.1.0"
  },
  "dependencies": {
    "-": "0.0.1",
    "@fastify/cookie": "9.2.0",
    "@fastify/jwt": "7.2.4",
    "@prisma/client": "5.7.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "fastify": "4.25.2",
    "prisma": "5.7.1",
    "zod": "3.22.4"
  }
}
